    "edinburgh_questionnaire": {
      "id": "C1T15E",
      "title": "Bloc C — Questionnaire d'Édimbourg",
      "description": "Questionnaire de claudication intermittente",
      "type": "questionnaire",
      "required": false,
      "condition": {
        "anyOf": ["C1T14E01_05", "C1T14E01_06"]
      },
      "questions": [
        {
          "id": "C1T15E01",
          "label": "Ressentez-vous une douleur ou une gêne dans une jambe lorsque vous marchez ?",
          "type": "single_choice",
          "required": true,
          "options": [
            {
              "id": "C1T15E01_yes",
              "label": "Oui"
            },
            {
              "id": "C1T15E01_no",
              "label": "Non"
            },
            {
              "id": "C1T15E01_cant_walk",
              "label": "Ne marche pas"
            }
          ],
          "conditional_questions": {
            "condition": "C1T15E01_yes",
            "questions": [
              {
                "id": "C1T15E02",
                "label": "Cette douleur commence-t-elle parfois à se manifester quand vous êtes debout immobile ou assis ?",
                "type": "single_choice",
                "required": true,
                "options": [
                  {
                    "id": "C1T15E02_yes",
                    "label": "Oui"
                  },
                  {
                    "id": "C1T15E02_no",
                    "label": "Non"
                  }
                ]
              },
              {
                "id": "C1T15E03",
                "label": "Ressentez-vous cette douleur quand vous montez une côte ou quand vous marchez vite ?",
                "type": "single_choice",
                "required": true,
                "options": [
                  {
                    "id": "C1T15E03_yes",
                    "label": "Oui"
                  },
                  {
                    "id": "C1T15E03_no",
                    "label": "Non"
                  }
                ]
              },
              {
                "id": "C1T15E04",
                "label": "La ressentez-vous quand vous marchez d'un pas normal sur un terrain plat ?",
                "type": "single_choice",
                "required": true,
                "options": [
                  {
                    "id": "C1T15E04_yes",
                    "label": "Oui"
                  },
                  {
                    "id": "C1T15E04_no",
                    "label": "Non"
                  }
                ]
              },
              {
                "id": "C1T15E05",
                "label": "Que devient la douleur si vous vous arrêtez ?",
                "type": "single_choice",
                "required": true,
                "options": [
                  {
                    "id": "C1T15E05_persists",
                    "label": "Elle persiste habituellement plus de 10 minutes"
                  },
                  {
                    "id": "C1T15E05_disappears",
                    "label": "Elle disparaît habituellement en 10 minutes ou moins"
                  }
                ]
              },
              {
                "id": "C1T15E06",
                "label": "Où ressentez-vous cette douleur ou cette gêne ?",
                "type": "multiple_choice",
                "required": false,
                "options": [
                  {
                    "id": "C1T15E06_buttocks",
                    "label": "Fesses"
                  },
                  {
                    "id": "C1T15E06_thighs",
                    "label": "Cuisses"
                  },
                  {
                    "id": "C1T15E06_calves",
                    "label": "Mollets"
                  },
                  {
                    "id": "C1T15E06_joints",
                    "label": "Articulatio"
                  }
                ]
              }
            ]
          }
        }
      ],
      "interpretation": {
        "claudication_intermittent_forte": {
          "condition": "Q1=oui, Q2=non, Q3=oui, Q4=oui, Q5=≤10min, Q6=fesses_cuisses_mollets",
          "result": "Claudication intermittente forte"
        },
        "claudication_intermittent_faible": {
          "condition": "Q1=oui, Q2=non, Q3=oui, Q4=non, Q5=≤10min, Q6=fesses_cuisses_mollets",
          "result": "Claudication intermittente faible"
        }
      }
    },
    "ipscb": {
      "id": "C1T15D",
      "title": "Bloc D — IPSCB*",
      "description": "Indice de pression systolique cheville bras",
      "type": "calculation",
      "required": false,
      "condition": {
        "anyOf": ["C1T14E01_05", "C1T14E01_06"]
      },
      "video_link": "https://www.youtube.com/watch?v=dQw4w9WgXcQ",
      "measurements": [
        {
          "id": "C1T15D01",
          "label": "PAS*",
          "description": "Pression artérielle systolique la plus élevée des deux bras",
          "type": "number",
          "unit": "mmHg",
          "validation": {
            "min": 50,
            "max": 300,
            "step": 1
          },
          "required": false,
          "ui": {
            "component": "NumericInput",
            "placeholder": "mmHg"
          }
        },
        {
          "id": "C1T15D03",
          "label": "PAS artère tibiale postérieure droite",
          "type": "number",
          "unit": "mmHg",
          "validation": {
            "min": 50,
            "max": 300,
            "step": 1
          },
          "required": false,
          "ui": {
            "component": "NumericInput",
            "placeholder": "mmHg"
          }
        },
        {
          "id": "C1T15D04",
          "label": "PAS artère pédieuse droite",
          "type": "number",
          "unit": "mmHg",
          "validation": {
            "min": 50,
            "max": 300,
            "step": 1
          },
          "required": false,
          "ui": {
            "component": "NumericInput",
            "placeholder": "mmHg"
          }
        },
        {
          "id": "C1T15D05",
          "label": "PAS artère tibiale postérieure gauche",
          "type": "number",
          "unit": "mmHg",
          "validation": {
            "min": 50,
            "max": 300,
            "step": 1
          },
          "required": false,
          "ui": {
            "component": "NumericInput",
            "placeholder": "mmHg"
          }
        },
        {
          "id": "C1T15D06",
          "label": "PAS artère pédieuse gauche",
          "type": "number",
          "unit": "mmHg",
          "validation": {
            "min": 50,
            "max": 300,
            "step": 1
          },
          "required": false,
          "ui": {
            "component": "NumericInput",
            "placeholder": "mmHg"
          }
        }
      ],
      "calculation": {
        "formula": "PAS artère du membre inférieur / PAS la plus élevée des deux bras",
        "output_fields": {
          "C1T15D07": "IPSCB tibiale postérieure droite",
          "C1T15D08": "IPSCB pédieuse droite", 
          "C1T15D09": "IPSCB tibiale postérieure gauche",
          "C1T15D10": "IPSCB pédieuse gauche"
        },
        "label": "Calcul PAS de l'artère du membre inférieur divisé par la PAS la plus élevée des deux bras et le résultat",
        "rounding": 2
      },
      "results": [
        {
          "id": "C1T15D07",
          "type": "calculated",
          "label": "IPSCB tibiale postérieure droite",
          "formula": "C1T15D03 / C1T15D01",
          "unit": "",
          "ui": {
            "component": "ResultBadge",
            "help": "Résultat automatiquement calculé"
          }
        },
        {
          "id": "C1T15D08", 
          "type": "calculated",
          "label": "IPSCB pédieuse droite",
          "formula": "C1T15D04 / C1T15D01",
          "unit": "",
          "ui": {
            "component": "ResultBadge",
            "help": "Résultat automatiquement calculé"
          }
        },
        {
          "id": "C1T15D09",
          "type": "calculated", 
          "label": "IPSCB tibiale postérieure gauche",
          "formula": "C1T15D05 / C1T15D01",
          "unit": "",
          "ui": {
            "component": "ResultBadge",
            "help": "Résultat automatiquement calculé"
          }
        },
        {
          "id": "C1T15D10",
          "type": "calculated",
          "label": "IPSCB pédieuse gauche", 
          "formula": "C1T15D06 / C1T15D01",
          "unit": "",
          "ui": {
            "component": "ResultBadge",
            "help": "Résultat automatiquement calculé"
          }
        }
      ],
      "interpretation": {
        "indeterminate": {
          "condition": "> 1.40",
          "label": "Indéterminé car artères non compressibles",
          "description": "chiffre si > 1,40 = indéterminé car artères non compressibles",
          "color": "#FF9800"
        },
        "normal": {
          "condition": "1.0 - 1.4",
          "label": "Normal",
          "description": "chiffre si 1,0 à 1,4 = normal",
          "color": "#4CAF50"
        },
        "borderline": {
          "condition": "0.9 - 0.99",
          "label": "Limite",
          "description": "chiffre si 0,9 à 0,99 = limite",
          "color": "#FF9800"
        },
        "mild": {
          "condition": "0.7 - 0.89",
          "label": "Anormal, atteinte légère",
          "description": "chiffre si 0,7 à 0,89 = anormal, atteinte légère",
          "color": "#F44336"
        },
        "moderate": {
          "condition": "0.4 - 0.69",
          "label": "Anormal, atteinte modéré",
          "description": "chiffre si 0,4 à 0,69 = anormal, atteinte modéré",
          "color": "#F44336"
        },
        "severe": {
          "condition": "< 0.4",
          "label": "***Anormal, atteinte sévère",
          "description": "chiffre si <0,4 = ***anormal, atteinte sévère",
          "color": "#F44336",
          "legend": "alert",
          "red_flag": true
        }
      }
    }
